CCXXXX = g++xxxx
MAKE_DEPEND := $(CC) -M
MAKE_DEPEND_R = $(CC) -M
MAKE_DEPEND_Q ?= $(CCXXXX) -M


all:
	@echo $(MAKE_DEPEND)
	@echo $(MAKE_DEPEND_R)
	@echo $(MAKE_DEPEND_Q)

CC = gcc 
CCXXXX = gccxxxx




